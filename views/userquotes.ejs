
<%- include('./partials/header', { title: `Quotations - ${subject.username}'s quotes`}) %>

<div class="center">
    <h1 class="supertitle"><a href="/user/profile:<%= subject.username %>"><%= subject.username %></a>'s quotes!</h1>
<% for (quote of quotes) { %>
    <%- include('./partials/quote', { quote }) %>
    <br>
    <br>
<% } %>
<% if (quotes.length == 0) { %>
    <h2><%= subject.username %> has not made any quotes yet... L!!!</h2>
<% } %>
</div>

<%- include('./partials/footer') %>